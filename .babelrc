{
  "presets": [
    ["@babel/preset-env",{
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }]
  ],
  "plugins": ["transform-vue-jsx", "@babel/plugin-transform-runtime","@babel/plugin-transform-async-to-generator","@babel/plugin-syntax-async-generators"],
  "env": {
    "test": {
      "presets": ["@babel/preset-env"],
      "plugins": ["transform-vue-jsx", "transform-es2015-modules-commonjs", "dynamic-import-node"]
    }
  }
}
